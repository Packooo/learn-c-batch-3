#include <iostream>
using namespace std;

int main(){
	int a=7, b=5;
	
	cout<<"Hasil a + b = " <<a + b <<endl;
	cout<<"Hasil a - b = " <<b - b <<endl;
	cout<<"Hasil a * b = " <<b * b <<endl;
	cout<<"Hasil a / b = " <<b / b <<endl;
	cout<<"Hasil a % b = " <<b % b <<endl;
}


